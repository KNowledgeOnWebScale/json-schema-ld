@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <http://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <http://example.org/gbfs#>.

gbfs:GBFSShape rdf:type sh:NodeShape;
    sh:property [
  sh:path gbfs:last_updated;
  sh:minInclusive 0;
  sh:maxInclusive 1924988399;
  sh:datatype xsd:integer
], [
  sh:path gbfs:ttl;
  sh:minInclusive 0;
  sh:datatype xsd:integer
], [
  sh:path gbfs:bike_id;
  sh:datatype xsd:string
], [
  sh:path gbfs:lat;
  sh:minInclusive -90;
  sh:maxInclusive 90;
  sh:datatype xsd:decimal
], [
  sh:path gbfs:lon;
  sh:minInclusive -180;
  sh:maxInclusive 180;
  sh:datatype xsd:decimal
], [
  sh:path gbfs:is_reserved;
  sh:or ([ sh:datatype xsd:boolean ] [ sh:datatype xsd:decimal ])
], [
  sh:path gbfs:is_disabled;
  sh:or ([ sh:datatype xsd:boolean ] [ sh:datatype xsd:decimal ])
].
