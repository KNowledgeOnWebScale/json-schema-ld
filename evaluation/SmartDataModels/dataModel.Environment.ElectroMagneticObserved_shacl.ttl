<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/data-models/Environment/ElectroMagneticObserved/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. Model:'https://schema.org/Text'. It has to be ElectroMagneticObserved";
  sh:name "type";
  sh:in (sdm:ElectroMagneticObserved);
  sh:minCount 1;
  sh:maxCount 1
], [
  sh:path sdm:refDevice;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:refPointOfInterest;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:dateObserved;
  sh:description "Property. Model:'https://schema.org/DateTime'. Date and time of this observation represented by an ISO8601 UTC format. It can be represented by an specific time instant or by an ISO8601 interval to separate attributes `dateObservedFrom`, `dateObservedTo`. ";
  sh:name "dateObserved";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:dateTime
], [
  sh:path sdm:dateObservedFrom;
  sh:description "Property. Model:'https://schema.org/DateTime'. Observation period: Start date and time in an ISO8601 UTC format. The attribute can be used in addition to the `dateObserved` attribute when it corresponds to a time interval to be highlighted. ";
  sh:name "dateObservedFrom";
  sh:datatype xsd:dateTime
], [
  sh:path sdm:dateObservedTo;
  sh:description "Property. Model:'https://schema.org/'. Observation period: End date and time in an ISO8601 UTC format. The attribute can be used in addition to the `dateObserved` attribute when it corresponds to a time interval to be highlighted.";
  sh:name "dateObservedTo";
  sh:datatype xsd:dateTime
], [
  sh:path sdm:eMF;
  sh:description "Property. Model:'https://schema.org/Number'. Level corresponding to the observed survey. The unit code (text) is given using the [UN/CEFACT Common Codes](http://wiki.goodrelations-vocabulary.org/Documentation/UN/CEFACT_Common_Codes). For instance, **MHz** represents Mega Hertz. ";
  sh:name "eMF";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:reliability;
  sh:description "Property. Model:'https://schema.org/'. Percent for confidence Factor. The unit code (text) is given using the [UN/CEFACT Common Codes](http://wiki.goodrelations-vocabulary.org/Documentation/UN/CEFACT_Common_Codes). For instance, **P1** represents Percent. ";
  sh:name "reliability";
  sh:minInclusive 0;
  sh:maxInclusive 1;
  sh:datatype xsd:decimal
].
