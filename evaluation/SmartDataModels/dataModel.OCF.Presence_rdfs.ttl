<https://smart-data-models.github.io/dataModel.IoTDataModels/Presence/schema.json> dcterms:title "Presence Sensor";
    dcterms:description "Smart Data Models Program adaptation of the original IoTData data Models. This Resource describes whether presence has been sensed or not. The Property 'value' is a boolean. A value of 'true' means that presence has been sensed. A value of 'false' means that presence not been sensed.".
sdm:rt rdf:type rdf:Property;
    rdfs:label "rt";
    rdfs:domain <https://smart-data-models.github.io/dataModel.IoTDataModels/Presence/schema.json>.
sdm:Rt rdf:type skos:ConceptScheme.
sdm:rt rdfs:range [
  owl:oneOf (sdm:oic.r.sensor.presence)
].
sdm:oic.r.sensor.presence rdf:type skos:Concept;
    skos:inScheme sdm:Rt;
    rdfs:label "oic.r.sensor.presence".
sdm:value dcterms:description "Property. The presences sensor, true = precense sensed, false = precensenot sensed.";
    rdf:type rdf:Property;
    rdfs:label "value";
    rdfs:domain <https://smart-data-models.github.io/dataModel.IoTDataModels/Presence/schema.json>;
    rdfs:range xsd:boolean.
sdm:n dcterms:description "Property. Friendly name of the Resource";
