@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix gbfsshape: <https://w3id.org/gbfs/shapes/free_bike_statusjson#>.

gbfsshape:free_bike_statusjsonShape rdf:type sh:NodeShape;
    sh:targetClass <https://github.com/NABSA/gbfs/blob/v1.0/gbfs.md#free_bike_statusjson>;
    sh:property [
  sh:path gbfs:last_updated;
  sh:description "Last time the data in the feed was updated in POSIX time.";
  sh:name "last_updated";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:maxInclusive 1924988399;
  sh:datatype xsd:integer
], [
  sh:path gbfs:ttl;
  sh:description "Number of seconds before the data in the feed will be updated again (0 if the data should always be refreshed).";
  sh:name "ttl";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:datatype xsd:integer
], [
  sh:path gbfs:data;
  sh:description "Array that contains one object per bike as defined below.";
  sh:name "data";
  sh:minCount 1;
  sh:maxCount 1
].
gbfs:DatumShape rdf:type sh:NodeShape;
    sh:targetClass gbfs:Datum;
    sh:property [
  sh:path gbfs:bikes;
  sh:name "bikes";
  sh:minCount 1;
  sh:maxCount 1
].
gbfs:BikeShape rdf:type sh:NodeShape;
    sh:targetClass gbfs:Bike;
    sh:property [
  sh:path gbfs:bike_id;
  sh:description "Rotating (as of v2.0) identifier of a vehicle.";
  sh:name "bike_id";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:string
], [
  sh:path gbfs:lat;
  sh:description "The latitude of the vehicle.";
  sh:name "lat";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive -90;
  sh:maxInclusive 90;
  sh:datatype xsd:decimal
], [
  sh:path gbfs:lon;
  sh:description "The longitude of the vehicle.";
  sh:name "lon";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive -180;
  sh:maxInclusive 180;
  sh:datatype xsd:decimal
], [
  sh:path gbfs:is_reserved;
  sh:xone ([ sh:datatype xsd:boolean ] [ sh:datatype xsd:decimal ])
], [
  sh:path gbfs:is_disabled;
  sh:description "Is the vehicle currently disabled (broken)?";
  sh:name "is_disabled";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:boolean
].
