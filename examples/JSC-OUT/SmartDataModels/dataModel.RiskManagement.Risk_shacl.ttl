@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://raw.githubusercontent.com/smart-data-models/dataModel.RiskAssessment/master/Risk/schema.json>.
sdmshape:schema.jsonShape sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI-LD Entity Type. It must be equal to Risk.";
  sh:name "type";
  sh:in (sdm:Risk);
  sh:minCount 1;
  sh:maxCount 1
], [
  sh:path sdm:validTo;
  sh:description "Property. Model:'https://schema.org/DateTime'. The time at which the risk materialization is started.";
  sh:name "validTo";
  sh:datatype xsd:dateTime
], [
  sh:path sdm:validFrom;
  sh:description "Property. Model:'https://schema.org/DateTime'. The time at which the risk materialization is finished.";
  sh:name "validFrom";
  sh:datatype xsd:dateTime
], [
  sh:path sdm:consequence;
  sh:description "Property. The consequence caused by a risk materialization. Enum:'quality, quantity, reputation'";
  sh:name "consequence";
  sh:in (sdm:quality sdm:quantity sdm:reputation)
], [
  sh:path sdm:description;
  sh:description "Property. The risk description in a natural language";
  sh:name "description";
  sh:datatype xsd:string
], [
  sh:path sdm:event;
  sh:description "Property. Occurrence or change of a particular set of circumstances. Enum:'destruction, interruption, manipulation, pollution'";
  sh:name "event";
  sh:in (sdm:destruction sdm:interruption sdm:manipulation sdm:pollution)
], [
  sh:path sdm:threat;
  sh:description "Property. The potential cause of an incident that may result in a breach of information security or compromise business operations. Enum:'cyber, cyber-physical, physical'";
  sh:name "threat";
  sh:in (sdm:cyber sdm:cyber-physical sdm:physical)
], [
  sh:path sdm:severity;
  sh:description "Property. The impact generated by the associated Risk. Enum:'LOW, MEDIUM, HIGH'";
  sh:name "severity";
  sh:in (sdm:LOW sdm:MEDIUM sdm:HIGH)
], [
  sh:path sdm:isOutputOf;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:sourceRisk;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:affects;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:mitigatedBy;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:linkTo;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
].
