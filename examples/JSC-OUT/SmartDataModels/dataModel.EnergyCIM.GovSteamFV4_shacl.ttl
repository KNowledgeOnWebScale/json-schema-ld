  sh:name "yhpmx";
  sh:datatype xsd:decimal
], [
  sh:path sdm:ympmn;
  sh:description "Property. Model:'https://schema.org/Number'. Minimum intercept valve position (Ympmn).  Typical Value = 0. Default: 0.0";
  sh:name "ympmn";
  sh:datatype xsd:decimal
], [
  sh:path sdm:ympmx;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum intercept valve position (Ympmx).  Typical Value = 1.1. Default: 0.0";
  sh:name "ympmx";
  sh:datatype xsd:decimal
].
