  sh:datatype xsd:decimal
], [
  sh:path sdm:kit;
  sh:description "Property. Model:'https://schema.org/Number'. Integral gain of electro-hydraulic regulator (Kit).  Typical Value = 0.04. Default: 0.0";
  sh:name "kit";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kmp1;
  sh:description "Property. Model:'https://schema.org/Number'. First gain coefficient of  intercept valves characteristic (Kmp1).  Typical Value = 0.5. Default: 0.0";
