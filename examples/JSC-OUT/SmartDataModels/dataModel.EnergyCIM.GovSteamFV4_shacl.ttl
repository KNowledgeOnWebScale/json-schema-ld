<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModels.CIMEnergyClasses/GovSteamFV4/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI type. It has to be GovSteamFV4";
  sh:name "type";
  sh:in (sdm:GovSteamFV4)
], [
  sh:path sdm:cpsmn;
  sh:description "Property. Model:'https://schema.org/Number'. Minimum value of pressure regulator output (Cpsmn).  Typical Value = -1. Default: 0.0";
  sh:name "cpsmn";
  sh:datatype xsd:decimal
], [
  sh:path sdm:cpsmx;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum value of pressure regulator output (Cpsmx).  Typical Value = 1. Default: 0.0";
  sh:name "cpsmx";
  sh:datatype xsd:decimal
], [
  sh:path sdm:crmn;
  sh:description "Property. Model:'https://schema.org/Number'. Minimum value of regulator set-point (Crmn).  Typical Value = 0. Default: 0.0";
  sh:name "crmn";
  sh:datatype xsd:decimal
], [
  sh:path sdm:crmx;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum value of regulator set-point (Crmx).  Typical Value = 1.2. Default: 0.0";
  sh:name "crmx";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kdc;
  sh:description "Property. Model:'https://schema.org/Number'. Derivative gain of pressure regulator (Kdc).  Typical Value = 1. Default: 0.0";
  sh:name "kdc";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kf1;
  sh:description "Property. Model:'https://schema.org/Number'. Frequency bias (reciprocal of droop) (Kf1).  Typical Value = 20. Default: 0.0";
