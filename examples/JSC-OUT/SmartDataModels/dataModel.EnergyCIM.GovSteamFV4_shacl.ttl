  sh:name "kpp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kpt;
  sh:description "Property. Model:'https://schema.org/Number'. Proportional gain of electro-hydraulic regulator (Kpt).  Typical Value = 0.3. Default: 0.0";
  sh:name "kpt";
  sh:datatype xsd:decimal
], [
  sh:path sdm:krc;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum variation of fuel flow (Krc).  Typical Value = 0.05. Default: 0.0";
