  sh:datatype xsd:decimal
], [
  sh:path sdm:kf3;
  sh:description "Property. Model:'https://schema.org/Number'. Frequency control (reciprocal of droop) (Kf3).  Typical Value = 20. Default: 0.0";
  sh:name "kf3";
  sh:datatype xsd:decimal
], [
  sh:path sdm:khp;
  sh:description "Property. Model:'https://schema.org/Number'. Fraction  of total turbine output generated by HP part (Khp).  Typical Value = 0.35. Default: 0.0";
  sh:name "khp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kic;
  sh:description "Property. Model:'https://schema.org/Number'. Integral gain of pressure regulator (Kic).  Typical Value = 0.0033. Default: 0.0";
