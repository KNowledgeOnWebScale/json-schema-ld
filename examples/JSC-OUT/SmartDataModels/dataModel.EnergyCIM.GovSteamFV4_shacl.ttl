], [
  sh:path sdm:svmn;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum regulator gate closing velocity (Svmn).  Typical Value = -0.0333. Default: 0.0";
  sh:name "svmn";
  sh:datatype xsd:decimal
], [
  sh:path sdm:svmx;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum regulator gate opening velocity (Svmx).  Typical Value = 0.0333. Default: 0.0";
  sh:name "svmx";
  sh:datatype xsd:decimal
], [
  sh:path sdm:ta;
  sh:description "Property. Model:'https://schema.org/Number'. Control valves rate opening time (Ta).  Typical Value = 0.8. Default: 0";
  sh:name "ta";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tam;
  sh:description "Property. Model:'https://schema.org/Number'. Intercept valves rate opening time (Tam).  Typical Value = 0.8. Default: 0";
  sh:name "tam";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tc;
  sh:description "Property. Model:'https://schema.org/Number'. Control valves rate closing time (Tc).  Typical Value = 0.5. Default: 0";
  sh:name "tc";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tcm;
  sh:description "Property. Model:'https://schema.org/Number'. Intercept valves rate closing time (Tcm).  Typical Value = 0.5. Default: 0";
  sh:name "tcm";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tdc;
