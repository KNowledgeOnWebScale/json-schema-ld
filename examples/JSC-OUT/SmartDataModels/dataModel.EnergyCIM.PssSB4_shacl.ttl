  sh:datatype xsd:decimal
], [
  sh:path sdm:tx2;
  sh:description "Property. Model:'https://schema.org/Number'. Time constant (Tx2). Default: 0";
  sh:name "tx2";
  sh:datatype xsd:decimal
], [
  sh:path sdm:vsmax;
  sh:description "Property. Model:'https://schema.org/Number'. Limiter (Vsmax). Default: 0.0";
  sh:name "vsmax";
  sh:datatype xsd:decimal
], [
