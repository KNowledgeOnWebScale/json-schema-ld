@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModel.OCF/ContinuousGlucoseMeterThreshold/schema.json>.
sdm:schema.jsonShape sh:property [
  sh:path sdm:plow;
  sh:description "Property. This Property describes the Patient low threshold (mg/dL)";
  sh:name "plow";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:phigh;
  sh:description "Property. This Property describes the Patient high threshold (mg/dL)";
  sh:name "phigh";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:dhypo;
  sh:description "Property. This Property describes the Device hypoglycemia threshold (mg/dL)";
  sh:name "dhypo";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:dhyper;
  sh:description "Property. This Property describes the Device hyperglycemia threshold (mg/dL)";
  sh:name "dhyper";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:gir;
  sh:description "Property. This Property describes the Glucose Increase rate of change threshold (%)";
  sh:name "gir";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:gdr;
  sh:description "Property. This Property describes the Glucose Decrease rate of change threshold (%)";
  sh:name "gdr";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:rt;
  sh:name "rt";
  sh:in (sdm:oic.r.cgm.threshold)
], [
  sh:path sdm:n;
  sh:description "Property. Friendly name of the Resource";
  sh:name "n";
  sh:maxLength 64;
  sh:datatype xsd:string
], [
  sh:path sdm:if;
  sh:name "if";
  sh:in (sdm:oic.if.rw sdm:oic.if.baseline)
], [
  sh:path sdm:range;
  sh:name "range";
  sh:datatype xsd:decimal
], [
  sh:path sdm:step;
  sh:description "Property. Step value across the defined range an integer when the range is a number.  This is the increment for valid values across the range; so if range is 0.0..10.0 and step is 2.5 then valid values are 0.0,2.5,5.0,7.5,10.0.";
  sh:name "step";
  sh:datatype xsd:decimal
], [
  sh:path sdm:precision;
  sh:description "Property. When exposed the value in 'precision' provides a +/- tolerance against the Properties in the Resource. Thus if a Property is UPDATED to a value and that Property then RETRIEVED, the RETRIEVED value is valid if in the range of the set value +/- precision";
  sh:name "precision";
  sh:datatype xsd:decimal
], [
  sh:path sdm:type;
  sh:description "Property. NGSI entity type. It has to be ContinuousGlucoseMeterThreshold";
  sh:name "type";
  sh:in (sdm:ContinuousGlucoseMeterThreshold);
  sh:minCount 1;
  sh:maxCount 1
].
