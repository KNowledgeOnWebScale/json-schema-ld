  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:string
], [
  sh:path sdm:numberOfRow;
  sh:description "Property. Number of Rows allowing the reading of the `rainFallradarContent` attribute. Model:'https://schema.org/Number'";
