@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModels.CIMEnergyClasses/GovSteamEU/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI type. It has to be GovSteamEU";
  sh:name "type";
  sh:in (sdm:GovSteamEU)
], [
  sh:path sdm:chc;
  sh:description "Property. Model:'https://schema.org/Number'. Control valves rate closing limit (Chc).  Unit = PU/sec.  Typical Value = -3.3. Default: 0.0";
  sh:name "chc";
  sh:datatype xsd:decimal
], [
  sh:path sdm:cho;
  sh:description "Property. Model:'https://schema.org/Number'. Control valves rate opening limit (Cho).  Unit = PU/sec.  Typical Value = 0.17. Default: 0.0";
  sh:name "cho";
  sh:datatype xsd:decimal
], [
  sh:path sdm:cic;
  sh:description "Property. Model:'https://schema.org/Number'. Intercept valves rate closing limit (Cic).  Typical Value = -2.2. Default: 0.0";
  sh:name "cic";
  sh:datatype xsd:decimal
], [
  sh:path sdm:cio;
  sh:description "Property. Model:'https://schema.org/Number'. Intercept valves rate opening limit (Cio).  Typical Value = 0.123. Default: 0.0";
  sh:name "cio";
  sh:datatype xsd:decimal
], [
  sh:path sdm:db1;
  sh:description "Property. Model:'https://schema.org/Number'. Dead band of the frequency corrector (db1).  Typical Value = 0. Default: 0.0";
  sh:name "db1";
  sh:datatype xsd:decimal
], [
  sh:path sdm:db2;
  sh:description "Property. Model:'https://schema.org/Number'. Dead band of the speed governor (db2).  Typical Value = 0.0004. Default: 0.0";
  sh:name "db2";
  sh:datatype xsd:decimal
], [
  sh:path sdm:hhpmax;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum control valve position (Hhpmax).  Typical Value = 1. Default: 0.0";
  sh:name "hhpmax";
  sh:datatype xsd:decimal
], [
  sh:path sdm:ke;
  sh:description "Property. Model:'https://schema.org/Number'. Gain of the power controller (Ke).  Typical Value = 0.65. Default: 0.0";
  sh:name "ke";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kfcor;
  sh:description "Property. Model:'https://schema.org/Number'. Gain of the frequency corrector (Kfcor).  Typical Value = 20. Default: 0.0";
  sh:name "kfcor";
  sh:datatype xsd:decimal
], [
  sh:path sdm:khp;
  sh:description "Property. Model:'https://schema.org/Number'. Fraction of total turbine output generated by HP part (Khp).  Typical Value = 0.277. Default: 0.0";
  sh:name "khp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:klp;
  sh:description "Property. Model:'https://schema.org/Number'. Fraction of total turbine output generated by HP part (Klp).  Typical Value = 0.723. Default: 0.0";
  sh:name "klp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kwcor;
  sh:description "Property. Model:'https://schema.org/Number'. Gain of the speed governor (Kwcor).  Typical Value = 20. Default: 0.0";
  sh:name "kwcor";
  sh:datatype xsd:decimal
], [
  sh:path sdm:mwbase;
  sh:description "Property. Model:'https://schema.org/Number'. Base for power values (MWbase) (>0).  Unit = MW. Default: 0.0";
  sh:name "mwbase";
  sh:datatype xsd:decimal
], [
  sh:path sdm:pmax;
  sh:description "Property. Model:'https://schema.org/Number'. Maximal active power of the turbine (Pmax).  Typical Value = 1. Default: 0.0";
  sh:name "pmax";
  sh:datatype xsd:decimal
], [
  sh:path sdm:prhmax;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum low pressure limit (Prhmax).  Typical Value = 1.4. Default: 0.0";
  sh:name "prhmax";
  sh:datatype xsd:decimal
], [
  sh:path sdm:simx;
  sh:description "Property. Model:'https://schema.org/Number'. Intercept valves transfer limit (Simx).  Typical Value = 0.425. Default: 0.0";
  sh:name "simx";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tb;
  sh:description "Property. Model:'https://schema.org/Number'. Boiler time constant (Tb).  Typical Value = 100. Default: 0";
  sh:name "tb";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tdp;
  sh:description "Property. Model:'https://schema.org/Number'. Derivative time constant of the power controller (Tdp).  Typical Value = 0. Default: 0";
  sh:name "tdp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:ten;
  sh:description "Property. Model:'https://schema.org/Number'. Electro hydraulic transducer (Ten).  Typical Value = 0.1. Default: 0";
  sh:name "ten";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tf;
  sh:description "Property. Model:'https://schema.org/Number'. Frequency transducer time constant (Tf).  Typical Value = 0. Default: 0";
  sh:name "tf";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tfp;
  sh:description "Property. Model:'https://schema.org/Number'. Time constant of the power controller (Tfp).  Typical Value = 0. Default: 0";
  sh:name "tfp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:thp;
  sh:description "Property. Model:'https://schema.org/Number'. High pressure (HP) time constant of the turbine (Thp).  Typical Value = 0.31. Default: 0";
  sh:name "thp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tip;
  sh:description "Property. Model:'https://schema.org/Number'. Integral time constant of the power controller (Tip).  Typical Value = 2. Default: 0";
  sh:name "tip";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tlp;
  sh:description "Property. Model:'https://schema.org/Number'. Low pressure(LP) time constant of the turbine (Tlp).  Typical Value = 0.45. Default: 0";
  sh:name "tlp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tp;
  sh:description "Property. Model:'https://schema.org/Number'. Power transducer time constant (Tp).  Typical Value = 0.07. Default: 0";
  sh:name "tp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:trh;
  sh:description "Property. Model:'https://schema.org/Number'. Reheater  time constant of the turbine (Trh).  Typical Value = 8. Default: 0";
  sh:name "trh";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tvhp;
  sh:description "Property. Model:'https://schema.org/Number'. Control valves servo time constant (Tvhp).  Typical Value = 0.1. Default: 0";
  sh:name "tvhp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tvip;
  sh:description "Property. Model:'https://schema.org/Number'. Intercept valves servo time constant (Tvip).  Typical Value = 0.15. Default: 0";
  sh:name "tvip";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tw;
  sh:description "Property. Model:'https://schema.org/Number'. Speed transducer time constant (Tw).  Typical Value = 0.02. Default: 0";
  sh:name "tw";
  sh:datatype xsd:decimal
], [
  sh:path sdm:wfmax;
  sh:description "Property. Model:'https://schema.org/Number'. Upper limit for frequency correction (Wfmax).  Typical Value = 0.05. Default: 0.0";
  sh:name "wfmax";
  sh:datatype xsd:decimal
], [
  sh:path sdm:wfmin;
  sh:description "Property. Model:'https://schema.org/Number'. Lower limit for frequency correction (Wfmin).  Typical Value = -0.05. Default: 0.0";
  sh:name "wfmin";
  sh:datatype xsd:decimal
], [
  sh:path sdm:wmax1;
  sh:description "Property. Model:'https://schema.org/Number'. Emergency speed control lower limit (wmax1).  Typical Value = 1.025. Default: 0.0";
  sh:name "wmax1";
  sh:datatype xsd:decimal
], [
  sh:path sdm:wmax2;
  sh:description "Property. Model:'https://schema.org/Number'. Emergency speed control upper limit (wmax2).  Typical Value = 1.05. Default: 0.0";
  sh:name "wmax2";
  sh:datatype xsd:decimal
], [
  sh:path sdm:wwmax;
  sh:description "Property. Model:'https://schema.org/Number'. Upper limit for the speed governor (Wwmax).  Typical Value = 0.1. Default: 0.0";
  sh:name "wwmax";
  sh:datatype xsd:decimal
], [
  sh:path sdm:wwmin;
  sh:description "Property. Model:'https://schema.org/Number'. Lower limit for the speed governor frequency correction (Wwmin).  Typical Value = -1. Default: 0.0";
  sh:name "wwmin";
  sh:datatype xsd:decimal
].
