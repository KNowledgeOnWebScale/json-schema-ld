    rdfs:range xsd:string.
<gbfs:https://w3id.org/gbfs#station_name> dcterms:description "Public name of the station.";
    rdf:type rdf:Property;
    rdfs:label "https://w3id.org/gbfs#station_name";
    rdfs:domain gbfs:Station;
    rdfs:range xsd:string.
gbfs:shortName dcterms:description "Short name or other type of identifier.";
