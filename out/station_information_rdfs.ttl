    rdfs:label "is_virtual_station";
    rdfs:domain gbfs:Station;
    rdfs:range xsd:boolean.
gbfs:stationArea dcterms:description "A multipolygon that describes the area of a virtual station (added in v2.1-RC).";
    rdf:type rdf:Property;
    rdfs:label "stationArea";
    rdfs:domain gbfs:Station;
    rdfs:range <http://www.opengis.net/ont/geosparql#Geometry>.
gbfs:parking_type dcterms:description "Type of parking station. Added in v2.3";
