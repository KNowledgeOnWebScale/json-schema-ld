@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <http://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.

<https://github.com/NABSA/gbfs/blob/v1.0/gbfs.md#system_alertsjsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://github.com/NABSA/gbfs/blob/v1.0/gbfs.md#system_alertsjson>;
    sh:property [
  sh:path <http://purl.org/dc/terms/modified>;
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:maxInclusive 1924988399;
  sh:datatype xsd:integer
], [
  sh:path gbfs:ttl;
  sh:description "Number of seconds before the data in the feed will be updated again (0 if the data should always be refreshed).";
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:datatype xsd:integer
].
gbfs:AlertShape rdf:type sh:NodeShape;
    sh:targetClass gbfs:Alert;
    sh:property [
  sh:path gbfs:alert_id;
  sh:description "Identifier for this alert.";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:string
], [
  sh:path <gbfs:https://w3id.org/gbfs#alert_type>;
  sh:description "Type of alert.";
  sh:in (gbfs:SYSTEM_CLOSURE gbfs:STATION_CLOSURE gbfs:STATION_MOVE gbfs:OTHER);
  sh:minCount 1;
  sh:maxCount 1
].
gbfs:AlertTimeShape rdf:type sh:NodeShape;
    sh:targetClass gbfs:AlertTime;
    sh:property [
  sh:path <gbfs:https://w3id.org/gbfs#alert_start>;
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path <gbfs:https://w3id.org/gbfs#alert_end>;
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path gbfs:station_ids;
  sh:datatype xsd:string
], [
  sh:path gbfs:region_ids;
  sh:datatype xsd:string
], [
  sh:path gbfs:url;
  sh:description "URL where the customer can learn more information about this alert.";
  sh:datatype xsd:string
], [
  sh:path gbfs:summary;
  sh:description "A short summary of this â€¦played to the customer.";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:string
], [
  sh:path gbfs:description;
  sh:description "Detailed description of the alert.";
  sh:datatype xsd:string
], [
  sh:path <http://purl.org/dc/terms/modified>;
  sh:minCount 1;
  sh:maxCount 1;
  sh:minInclusive 0;
  sh:maxInclusive 1924988399;
  sh:datatype xsd:integer
].
